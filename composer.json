{
    "name": "codehq-dk/repo-info",
    "description": "Repository Information. Use this to create a list of your code repositories with information defined by plugins",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "kodus/uuid-v4": "^1.2.0",
        "codehq-dk/repo-info-contracts" : "^1.0.0-alpha4",
        "codehq-dk/linux-bash-helper" : "^1.0.0-alpha2"
    },
    "autoload": {
        "psr-4": {
            "CodeHqDk\\RepositoryInformation\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CodeHqDk\\RepositoryInformation\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "slince/di" : "^3.2.0",
        "phpunit/phpunit": "^9.5",
        "lcobucci/clock": "^3.0.0",
        "codehq-dk/repo-info-example-plugin" : "^1.0.0-alpha4"
    },
    "scripts": {
        "test": "vendor/bin/phpunit tests/Unit",
        "test-report": "vendor/bin/phpunit tests/Unit --coverage-html tests/CoverageReport"
    }
}
